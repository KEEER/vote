{
  "code": "default",
  "default": true,
  "version": "0.0.1",
  "entryName": "theme-default",
  "provides": {
    "types": [
      "VCheckbox",
      "VRadio",
      "VText",
      "VTextarea"
    ],
    "questionConfig": {
      "VCheckbox": [
        {
          "type": "checkbox",
          "name": "multiline",
          "label": "theme.default.config.question.multiline",
          "default": true
        }
      ]
    },
    "formConfig": [
      {
        "type": "color",
        "name": "theme-default.text-color",
        "label": "theme.default.config.form.textColor",
        "default": "#ffffff"
      },
      {
        "type": "color",
        "name": "theme-default.bg-color",
        "label": "theme.default.config.form.bgColor",
        "default": "#000000"
      },
      {
        "type": "color",
        "name": "theme-default.theme-color",
        "label": "theme.default.config.form.themeColor",
        "default": "#005c5c"
      },
      {
        "type": "color",
        "name": "theme-default.secondary-color",
        "label": "theme.default.config.form.secondaryColor",
        "default": "#002d4d"
      }
    ],
    "hooks": [
      "form:update",
      "form:beforesubmit",
      "form:submit",
      "form:error",
      "form:mounted",
      "form:pageno",
      "form:updatevisibility",
      "form:validate",
      "question:update",
      "question:validate",
      "page:validate"
    ],
    "injections": [
      "form"
    ]
  }
}
