{
  "code": "basic",
  "default": true,
  "version": "0.0.1",
  "entryName": "theme-basic",
  "provides": {
    "types": [
      "VCheckbox",
      "VRadio",
      "VText",
      "VTextarea"
    ],
    "questionConfig": {
      "VCheckbox": [
        {
          "type": "checkbox",
          "name": "multiline",
          "label": "theme.basic.config.question.multiline",
          "default": true
        }
      ]
    },
    "formConfig": [
      {
        "type": "color",
        "name": "theme-basic.color",
        "label": "theme.basic.config.form.color",
        "default": "#000000"
      }
    ],
    "hooks": [
      "form:update",
      "form:beforesubmit",
      "form:submit",
      "form:error",
      "form:mounted",
      "form:pageno",
      "form:updatevisibility",
      "form:validate",
      "app:nodata",
      "question:update",
      "question:validate",
      "page:validate"
    ],
    "injections": [
      "form"
    ]
  }
}
