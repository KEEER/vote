{
  "name": "Sample",
  "description": "A sample plugin to demonstrate capabilities of plugins.",
  "code": "sample",
  "version": "0.0.1",
  "default": false,
  "required": false,
  "pro": false,
  "uses": {
    "types": [
      "VText"
    ],
    "hooks": [
      "question:update",
      "form:texts"
    ],
    "inject": {
      "form": {
        "js": "plugin-sample-form",
        "css": "plugin-sample-form"
      },
      "editor": {
        "js": "plugin-sample-editor",
        "css": "plugin-sample-editor"
      },
      "server": "server.js"
    }
  }
}
