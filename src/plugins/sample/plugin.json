{
  "code": "sample",
  "version": "0.0.1",
  "default": false,
  "required": false,
  "pro": false,
  "uses": {
    "types": [
      "VText"
    ],
    "hooks": [
      "question:update",
      "editor:appMounted",
      "editor:beforeRouterLoad",
      "editor:settingsMounted"
    ],
    "inject": {
      "form": {
        "js": "plugin-sample-form",
        "css": "plugin-sample-form"
      },
      "editor": {
        "js": "plugin-sample-editor",
        "css": "plugin-sample-editor"
      },
      "server": "server.js"
    }
  }
}
